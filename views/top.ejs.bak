<!DOCTYPE html>

<html>
  <head>
    <title>Usuarios</title>
    <link rel='stylesheet' href='/stylesheets/styleuser.css' />
  </head>
  <style>
  .contenedor-imagenes {
		display: flex;
		}
	table {
		border-collapse: collapse;
		width: 100%;
	}

	th, td {
		text-align: left;
		padding: 8px;
	}	

	tr:nth-child(even) {
		background-color: #D6EEEE;
	}
  </style>
  <body>
  
	<h2>Database Results</h2>
    
	
	
	<table class="tablaTop" border="1px" >
	<tr>
		<th>Role</th>
		<th>Name</th>
		<th>Tier</th>
		<th>Win Rate</th>
	</tr>
	
	<%results.forEach(function(r){ %>
		<%if(r.role == 'Top'){ %>
		<tr>
			<td><%= r.role %></td>
			<td><%= r.name %></td>
			<td><%= r.tier %></td>
			<td><%= r.wrate %></td>
		</tr>
		<% } %>
		<% }); %>
	</table>
	
	
	
  </body>
</html>
